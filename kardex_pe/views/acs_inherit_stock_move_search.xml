<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <data>

    <record id="acs_inherit_stock_move_search" model="ir.ui.view">
      <field name="name">acs_inherit_stock_move_search</field>
      <field name="model">stock.move.line</field>
      <field name="inherit_id" ref="stock.stock_move_line_view_search"/>
      <field name="arch" type="xml">
        <field name="picking_id" position="replace"></field>
        <field name="product_id" position="after">
          <field name="price_unit" string='Precio'/>
          <field name="doc_num" string="Ref. Movimiento"/>
          <field name="partner_name" string="Socio"/>
        </field>
      </field>
    </record>

  </data>
</odoo>
